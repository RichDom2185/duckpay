datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "foreignKeys"
}

generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/client"
}

model Account {
  id          String       @id @default(cuid())
  createdAt   DateTime     @default(now())
  deletedAt   DateTime?
}
